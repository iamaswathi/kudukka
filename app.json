{
  "expo": {
    "name": "kudukka",
    "slug": "my-savings-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mysavingsapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
          "image": "./assets/images/splashicon.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}